cordova build --release
jarsigner -verbose -sigalg SHA1withRSA -digestalg SHA1 -keystore android.keystore platforms/android/app/build/outputs/apk/release/app-release-unsigned.apk android-app-key
rm -rf platforms/android/app/build/outputs/apk/release/app-released.apk
zipalign -v 4  platforms/android/app/build/outputs/apk/release/app-release-unsigned.apk platforms/android/app/build/outputs/apk/release/app-released.apk